{
    "version": 2,
    "builds": [
        {
            "src": "next.config.js",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://backend-erp-warehouse.vercel.app/api/v1/:path*"
        }
    ]
}
